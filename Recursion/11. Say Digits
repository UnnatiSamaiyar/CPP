#include<bits/stdc++.h>
using namespace std;

int sayDigits(int n, string arr[]){
	if(n==0){
		return n;
	}
	
	int digit = n%10;
	n = n/10;
	
	sayDigits(n,arr);
	
	cout<<arr[digit]<<" ";
	return 1;
}

int main(){
	int n;
	cin>>n;
	cout<<endl;
	
	string arr[10] = {"zero","one","two","three","four","five","six","seven","eight","nine"};
	
	sayDigits(n,arr);
	cout<<endl;
}
