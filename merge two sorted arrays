#include<bits/stdc++.h>
using namespace std;
vector<int> mergeSortedArrays(vector<int>A,vector<int>B){
    vector<int>C;
    int i=0,j=0,k=0;
    while(i<A.size() && j<B.size()){
        if (A[i]<B[j]){
            C.push_back(A[i]);
            i++;
        }
        else{
            C.push_back(B[j]);
            j++;
        }
    }
    return C;
}
int main() {
    vector<int>A ={1,2,5,6};
    vector<int>B = {1,3,4,7};
    vector<int>C = mergeSortedArrays(A,B);
    for(int i=0;i<C.size();i++){
        cout<<C[i]<<" ";
    }
}
